{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@my-guardian-api/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@my-guardian-api/database": "<rootDir>/../libs/database/src",
    "@my-guardian-api/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@my-guardian-api/common": "<rootDir>/../libs/common/src",
    "@my-guardian-api/mailsender/(.*)": "<rootDir>/../libs/mailsender/src/$1",
    "@my-guardian-api/mailsender": "<rootDir>/../libs/mailsender/src",
    "@my-guardian-api/authenticate/(.*)": "<rootDir>/../libs/authenticate/src/$1",
    "@my-guardian-api/authenticate": "<rootDir>/../libs/authenticate/src",
    "@my-guardian-api/google-map/(.*)": "<rootDir>/../libs/google-map/src/$1",
    "@my-guardian-api/google-map": "<rootDir>/../libs/google-map/src",
    "@app/firebase/(.*)": "<rootDir>/../libs/firebase/src/$1",
    "@app/firebase": "<rootDir>/../libs/firebase/src"
  }
}