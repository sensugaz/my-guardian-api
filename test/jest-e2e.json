{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@my-guardian-api/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@my-guardian-api/database": "<rootDir>/../libs/database/src",
    "@my-guardian-api/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@my-guardian-api/common": "<rootDir>/../libs/common/src",
    "@my-guardian-api/mailsender/(.*)": "<rootDir>/../libs/mailsender/src/$1",
    "@my-guardian-api/mailsender": "<rootDir>/../libs/mailsender/src"
  }
}